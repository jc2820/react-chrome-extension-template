{
  "short_name": "Reader Popup",
  "name": "Reader Popup",
  "icons": {
    "16": "favicon.ico",
    "48": "logo192.png",
    "128": "logo512.png"
  },
  "content_security_policy": "script-src 'self' 'sha256-ANrZvsTgRmW2dRQu0/gTb5L+lkHlvnDrjFeNgAu/Fww='; object-src 'self'",
  "permissions": [],
  "manifest_version": 2,
  "version": "0.0.1",
  "browser_action": {
    "default_title": "Reader popup"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["/static/css/main.css", "/static/css/content.css"],
      "js": ["/static/js/content.js"]
    }
  ],
  "background": {
    "scripts": ["app/background.js"]
  }
}
